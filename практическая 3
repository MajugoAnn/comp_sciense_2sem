x = float(input("Введите значение x: "))
y = float(input("Введите значение y: "))
z = float(input("Введите значение z: "))
min_term = min(y**2,(x+y-z)/y) 
max_term = max((5+2*x)/min_term)
u = max_term
print("Значение x:", x)
print("Значение y:", y)
print("Значение z:", z)
print("Значение max{(5+2*x)/min_term}:", max_term)
print("Значение min{y**2,(x+y-z)/y}:", min_term)
print("Значение u:", u)
